{
  "kind": "collectionType",
  "collectionName": "webinars",
  "info": {
    "singularName": "webinar",
    "pluralName": "webinars",
    "displayName": "Webinar"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "number": {
      "type": "string"
    },
    "date": {
      "type": "string"
    },
    "time": {
      "type": "string"
    },
    "excerpt": {
      "type": "text"
    },
    "objectives": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "url": {
      "type": "text"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "webinar_date": {
      "type": "datetime",
      "required": false
    }
  }
}
